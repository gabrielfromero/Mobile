<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wolff.jackson.aula04_gabriel" >
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >




        <activity
            android:name="com.wolff.jackson.aula04_gabriel.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wolff.jackson.aula04_gabriel.ListarActivity"
            android:label="@string/title_activity_listar"
            android:parentActivityName="com.wolff.jackson.aula04_gabriel.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wolff.jackson.aula04_gabriel.MainActivity" />
        </activity>
        <activity
            android:name="com.wolff.jackson.aula04_gabriel.CadastrarActivity"
            android:label="@string/title_activity_cadastrar"
            android:parentActivityName="com.wolff.jackson.aula04_gabriel.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wolff.jackson.aula04_gabriel.MainActivity" />
        </activity>
        <activity
            android:name="com.wolff.jackson.aula04_gabriel.SobreActivity"
            android:label="@string/title_activity_sobre"
            android:parentActivityName="com.wolff.jackson.aula04_gabriel.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wolff.jackson.aula04_gabriel.MainActivity" />
        </activity>
    </application>

</manifest>
